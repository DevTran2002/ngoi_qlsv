<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="contract_type_view_tree" model="ir.ui.view" >
        <field name="name">Contract.type.view.tree</field>
        <field name="model">contract.type</field>
        <field name="arch" type="xml">
            <tree string="" editable="bottom">
                <field name="sequence" widget="handle"/>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="contract_type_action" model="ir.actions.act_window">
        <field name="name">Contract type</field>
        <field name="res_model">contract.type</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>
